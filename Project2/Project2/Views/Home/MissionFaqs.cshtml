@using Project2.Models.MissionSpecQuestions

@{
    ViewBag.Title = "MissionFaqs";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>MissionFaqs</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.missionName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.missionName)
            </td>
            <td>
                @Html.ActionLink("Add Owner", "AddOwner", new { id = item.carID })
            </td>
        </tr>
    }

</table>
